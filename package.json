{
  "name": "express-example",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "nodemon ./bin/www localhost 3000",
    "test": "npm run test-unit && npm run test-integration",
    "test-unit": "cross-env NODE_ENV=test ./node_modules/.bin/mocha test/unit/*.test.js",
    "test-integration": "cross-env NODE_ENV=test ./node_modules/.bin/mocha test/integration/*.test.js",
    "production": "cross-env NODE_ENV=production node ./bin/www"
  },
  "dependencies": {
    "bluebird": "^3.5.1",
    "body-parser": "^1.18.2",
    "cookie-parser": "^1.4.3",
    "debug": "^3.1.0",
    "ejs": "^2.5.7",
    "express": "^4.16.2",
    "express-ejs-layouts": "^2.3.1",
    "morgan": "^1.7.0",
    "mysql": "^2.15.0",
    "nodemon": "^1.14.10",
    "pg": "^6.1.0",
    "sequelize": "^4.38.0",
    "serve-favicon": "~2.5.0",
    "sqlite3": "^4.0.2"
  },
  "devDependencies": {
    "cross-env": "^3.1.3",
    "eslint": "^5.2.0",
    "eslint-config-airbnb-base": "^13.0.0",
    "eslint-plugin-import": "^2.13.0",
    "expect.js": "^0.3.1",
    "mocha": "^5.2.0",
    "supertest": "^3.1.0"
  }
}
